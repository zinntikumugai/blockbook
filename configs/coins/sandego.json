{
  "coin": {
    "name": "SanDeGo",
    "shortcut": "SDGO",
    "label": "SanDeGo",
    "alias": "sandego"
  },
  "ports": {
    "backend_rpc": 31933,
    "backend_message_queue": 0,
    "blockbook_internal": 9089,
    "blockbook_public": 9189
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-sandego",
    "package_revision": "satoshilabs-1",
    "system_user": "sandego",
    "version": "4.0.0.0",
    "binary_url": "https://github.com/sandegodev/SanDeGo/releases/download/4.0.0/sandego-qt-linux-v4.0.0.tar.gz",
    "verification_type": "sha256",
    "verification_source": "4E3B84AB0B59C75967C72CCF5D1EC0CB9727FEB48FD2B7B513250BC209A9FD75",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [
      "bin/sandego-qt"
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/sandegod -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "bitcoin_like.conf",
    "client_config_file": "bitcoin_like_client.conf",
    "additional_params": {
      "whitelist": "127.0.0.1"
    }
  },
  "blockbook": {
    "package_name": "blockbook-sandego",
    "system_user": "blockbook-sandego",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "xpub_magic": 76067358,
      "xpub_magic_segwit_p2sh": 77429938,
      "slip44": 15845,
      "additional_params": {}
    }
  },
  "meta": {
    "package_maintainer": "zinntikumugai",
    "package_maintainer_email": "uesitananame55@gmail.com"
  }
}